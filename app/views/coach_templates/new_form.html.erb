<h1>
  Add a new coach
</h1>


<hr>

<form action="/create_coach" method="post">

  <div class="form-group">
    <label for="inviter_id">
      Inviter
    </label>

    <input type="text" id="inviter_id" name="inviter_id" class="form-control">
  </div>

  <div class="form-group">
    <label for="invitee_id">
      Invitee
    </label>

    <input type="text" id="invitee_id" name="invitee_id" class="form-control">
  </div>

  <button class="btn btn-block btn-outline-secondary">
    Create coach
  </button>
</form>

<hr>

<a href="/coaches">
  Go back
</a>
